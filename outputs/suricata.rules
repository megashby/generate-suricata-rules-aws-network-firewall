alert dns any any -> any any (msg:"LOG traffic for domain example.com"; dns.query; content:"example.com"; nocase; sid:1; rev:1;)
drop tls any any -> any any (msg:"DROP traffic for domain example.com via tls"; tls.sni; content:"example.com"; nocase; sid:2; rev:1;)
pass http any any -> any any (msg:"PASS traffic for domain test.org via http"; http.host; content:"test.org";  sid:3; rev:1;)